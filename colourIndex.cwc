      "content": "var j, texte, values, list, data, index, r;\n\nfunction colourRandom() {\n  var num = Math.floor(Math.random() * Math.pow(2, 24));\n  return '#' + ('00000' + num.toString(16)).substr(-6);\n}\n\n/**\n * Décrire cette fonction…\n */\nfunction init() {\n  texte = '1=X,2=Y,3=rayon,4=couleur,5=XX,6=YY';\n  values = '150,200,30,98';\n  list = texte.split(',');\n  data = values.split(',');\n  list.push('var');\n  data.push(0);\n  index = [];\n  r = 0;\n  for (var count = 0; count < 10; count++) {\n    r = (typeof r == 'number' ? r : 0) + 4;\n    index.push(r);\n  }\n  command.writeLine(index);\n}\n\n\ninit();\nlet infinity_loop = function() {\n  try {\n  for (j = 1; j <= 30; j += 2) {\n    draw.rectangle(250, (j * 20), 40, j, (colourRandom()), (colourRandom()), 4);\n  }\n  } catch (err) {\n    return;\n  }\n  window.setTimeout(infinity_loop, 50);\n};\ninfinity_loop();\n\ndraw.rectangle(250, 200, j, j, (colourRandom()), (colourRandom()), 4);\n\nj;\n\ndraw.clear();\n\nj;\n\ndraw.clear();\n\nj * 15;\n",
