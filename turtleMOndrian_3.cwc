      "content": "var d, v, long, k, Lx, Ly, y, x, r, y1, x1;\n\nfunction mathRandomInt(a, b) {\n  if (a > b) {\n    // Swap a and b to ensure a is smaller.\n    var c = a;\n    a = b;\n    b = c;\n  }\n  return Math.floor(Math.random() * (b - a + 1) + a);\n}\n\nfunction colourRandom() {\n  var num = Math.floor(Math.random() * Math.pow(2, 24));\n  return '#' + ('00000' + num.toString(16)).substr(-6);\n}\n\n/**\n * Describe this function...\n */\nfunction turtle(d, v) {\n  long = mathRandomInt(20, 10);\n  v = v * mathRandomInt(-1, 1);\n  if (d == 1) {\n    y = Ly;\n    x = true ? Lx : v;\n    x1 = x + v * long;\n    y1 = y;\n    Lx = x1;\n  } else if (d == 2) {\n    x = Lx;\n    y = true ? y1 : v;\n    y1 = y + v * long;\n    Ly = y1;\n  }\n  draw.line(x, y, x1, y1, (colourRandom()), (false ? (60 / v) * 1.5 : r));\n  draw.circle(x, y, 6, '#ff9900', '#ff9900', 1);\n  r = (typeof r == 'number' ? r : 0) + -2;\n}\n\n/**\n * Describe this function...\n */\nfunction wait() {\n  k = 0;\n  var repeat_end = Math.pow(10,8);\n  for (var count2 = 0; count2 < repeat_end; count2++) {\n    k = (typeof k == 'number' ? k : 0) + 1;\n  }\n  draw.clear();\n}\n\n\nlet infinity_loop = function() {\n  try {\n  wait();\n  Lx = 200;\n  Ly = 200;\n  r = 20;\n  for (var count = 0; count < 4; count++) {\n    turtle(1, mathRandomInt(4, 6));\n    turtle(2, mathRandomInt(4, 6));\n  }\n  } catch (err) {\n    return;\n  }\n  window.setTimeout(infinity_loop, 50);\n};\ninfinity_loop();\n",
