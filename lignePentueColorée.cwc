      "content": "var dy, xx, y1, distance1, dx, yy, x1, x, y, k, fin, pente, i, c, a, distance2;\n\n/**\n * Décrire cette fonction…\n */\nfunction init() {\n  distance1 = 0;\n  k = 0;\n}\n\nfunction mathRandomInt(a, b) {\n  if (a > b) {\n    // Swap a and b to ensure a is smaller.\n    var c = a;\n    a = b;\n    b = c;\n  }\n  return Math.floor(Math.random() * (b - a + 1) + a);\n}\n\n/**\n * Décrire cette fonction…\n */\nfunction square() {\n  xx = (x1 - x) * (x1 - x);\n  yy = (y1 - y) * (y1 - y);\n  return Math.sqrt(xx + yy);\n}\n\nfunction colourRandom() {\n  var num = Math.floor(Math.random() * Math.pow(2, 24));\n  return '#' + ('00000' + num.toString(16)).substr(-6);\n}\n\n/**\n * Describe this function...\n */\nfunction ligne() {\n  y1 = mathRandomInt(20, 500);\n  x1 = mathRandomInt(20, 380);\n  draw.line(x, y, x1, y1, (colourRandom()), 21);\n  draw.circle(x, y, 12, '#000099', '#6633ff', 2);\n}\n\n/**\n * Describe this function...\n */\nfunction do2() {\n  dy = y1 - y;\n  dx = x1 - x;\n  pente = dy / dx;\n  a = Math.atan(1 / pente) / Math.PI * 180;\n  c = colourRandom();\n  while (0 == fin) {\n    distance1 = square();\n    if (dy < 0) {\n      x = (typeof x == 'number' ? x : 0) + 21 * Math.sin((180 + a) / 180 * Math.PI);\n      y = (typeof y == 'number' ? y : 0) + 21 * Math.cos((180 + a) / 180 * Math.PI);\n    } else {\n      x = (typeof x == 'number' ? x : 0) + 21 * Math.sin(a / 180 * Math.PI);\n      y = (typeof y == 'number' ? y : 0) + 21 * Math.cos(a / 180 * Math.PI);\n    }\n    distance2 = square();\n    if (distance2 < distance1) {\n      draw.circle(x, y, 8, c, '#ffffff', 1);\n    } else {\n      fin = 1;\n    }\n  }\n}\n\n/**\n * Describe this function...\n */\nfunction points() {\n  xx = x;\n  yy = y;\n  for (i = 1; i <= 4; i += 0.2) {\n    xx = (typeof xx == 'number' ? xx : 0) + 21 * Math.sin(a / 180 * Math.PI);\n    yy = (typeof yy == 'number' ? yy : 0) + 21 * Math.cos(a / 180 * Math.PI);\n    draw.circle(xx, yy, i, '#33ff33', '#ff0000', 3);\n  }\n}\n\n/**\n * Describe this function...\n */\nfunction direction() {\n  draw.line(x, y, (x + Math.sin(a / 180 * Math.PI) * 90), (y + Math.cos(a / 180 * Math.PI) * 90), '#330000', 3);\n}\n\n/**\n * Décrire cette fonction…\n */\nfunction texte() {\n  command.writeLine((['Pente=',Math.round(pente),'   /   Angle= ',Math.round(a),'   /   TO = ',Math.round(distance2),'   /   DD = ',Math.round(distance1),'',c].join('')));\n}\n\n\ninit();\nlet infinity_loop = function() {\n  try {\n  fin = 0;\n  k = (typeof k == 'number' ? k : 0) + 1;\n  if (0 == k % 7) {\n    x = mathRandomInt(20, 380);\n    y = mathRandomInt(20, 380);\n    ligne();\n    do2();\n  }\n  if (0 == k % 500) {\n    draw.clear();\n    k = 0;\n  }\n  } catch (err) {\n    return;\n  }\n  window.setTimeout(infinity_loop, 50);\n};\ninfinity_loop();\n",
