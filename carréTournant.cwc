      "content": "var a, c, b;\n\n/**\n * Décrire cette fonction…\n */\nfunction cerclepoint() {\n  draw.point(200, 200, '#ff00ff', 10);\n  c = 0;\n  for (var count = 0; count < 36; count++) {\n    c = (typeof c == 'number' ? c : 0) + 10;\n    draw.point((200 + Math.sin(c / 180 * Math.PI) * 100), (200 + Math.cos(c / 180 * Math.PI) * 100), '#ff00ff', 3);\n  }\n}\n\n/**\n * Décrire cette fonction…\n */\nfunction carr_C3_A9() {\n  for (var count2 = 0; count2 < 4; count2++) {\n    a = (typeof a == 'number' ? a : 0) + 90;\n    b = a + 90;\n    draw.line((200 + Math.sin(a / 180 * Math.PI) * 100), (200 + Math.cos(a / 180 * Math.PI) * 100), (200 + Math.sin(b / 180 * Math.PI) * 100), (200 + Math.cos(b / 180 * Math.PI) * 100), '#0000ff', 3);\n  }\n}\n\n\ndraw.clear();\n\na = 0;\nlet infinity_loop = function() {\n  try {\n  draw.point(400, 400, '#ff00ff', 5);\n  a = (typeof a == 'number' ? a : 0) + 5;\n  draw.clear();\n  carr_C3_A9();\n  cerclepoint();\n  } catch (err) {\n    return;\n  }\n  window.setTimeout(infinity_loop, 50);\n};\ninfinity_loop();\n",
