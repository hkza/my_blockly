      "content": "var d, v, long, k, y, x, y1, x1;\n\nfunction mathRandomInt(a, b) {\n  if (a > b) {\n    // Swap a and b to ensure a is smaller.\n    var c = a;\n    a = b;\n    b = c;\n  }\n  return Math.floor(Math.random() * (b - a + 1) + a);\n}\n\nfunction colourRandom() {\n  var num = Math.floor(Math.random() * Math.pow(2, 24));\n  return '#' + ('00000' + num.toString(16)).substr(-6);\n}\n\n/**\n * Describe this function...\n */\nfunction turtle(d, v) {\n  long = mathRandomInt(20, 10);\n  if (d == 1) {\n    y = mathRandomInt(20, 300);\n    x = v;\n    x1 = x + v * long;\n    y1 = y;\n  } else if (d == 2) {\n    x = x1;\n    y = false ? y1 : v;\n    y1 = y + v * long;\n  }\n  draw.line(x, y, x1, y1, (colourRandom()), (true ? (60 / v) * 1.5 : 5));\n}\n\n/**\n * Describe this function...\n */\nfunction wait() {\n  k = 0;\n  var repeat_end = Math.pow(10,8);\n  for (var count = 0; count < repeat_end; count++) {\n    k = (typeof k == 'number' ? k : 0) + 1;\n  }\n  draw.clear();\n}\n\n/**\n * Describe this function...\n */\nfunction do_something() {\n  let infinity_loop = function() {\n    try {\n    wait();\n    for (var count2 = 0; count2 < 4; count2++) {\n    }\n    } catch (err) {\n      return;\n    }\n    window.setTimeout(infinity_loop, 50);\n  };\n  infinity_loop();\n}\n\n\nturtle(1, mathRandomInt(1, 20));\nturtle(2, mathRandomInt(1, 20));\n",
