      "content": "var list, k, tempo, i;\n\n/**\n * Décrire cette fonction…\n */\nfunction tempo2() {\n}\n\nfunction listsRepeat(value, n) {\n  var array = [];\n  for (var i = 0; i < n; i++) {\n    array[i] = value;\n  }\n  return array;\n}\n\nfunction mathRandomInt(a, b) {\n  if (a > b) {\n    // Swap a and b to ensure a is smaller.\n    var c = a;\n    a = b;\n    b = c;\n  }\n  return Math.floor(Math.random() * (b - a + 1) + a);\n}\n\n\nlist;\n\nk = 0;\nlist = listsRepeat(0, 20);\nlet infinity_loop = function() {\n  try {\n  tempo = mathRandomInt(1, 100) % 8;\n  list[(tempo - 1)] = 1 + list[(tempo - 1)];\n  k = (typeof k == 'number' ? k : 0) + 1;\n  list[8] = k;\n  for (i = 1; i <= 20; i++) {\n    draw.rectangle((i * 20 + 60), 400, 15, (list[(i - 1)] * -1), '#00ff00', '#000000', 1);\n  }\n  } catch (err) {\n    return;\n  }\n  window.setTimeout(infinity_loop, 50);\n};\ninfinity_loop();\n\nlist[(tempo - 1)];\n\ntempo;\n\n1 + 1;\n\ncommand.writeLine(list);\n",
